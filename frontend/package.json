{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:analysis": "tsc -b && cross-env BUNDLE_ANALYSIS=true vite build",
    "build:prod": "tsc -b && vite build --mode production",
    "typecheck": "tsc --noEmit --pretty false",
    "typecheck:strict": "tsc --noEmit --strict --pretty false",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "analyze": "npm run build:analysis && start dist/bundle-analysis.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:verbose": "jest --verbose",
    "test:integration": "jest --testPathPattern=Integration.test.tsx --verbose",
    "test:backend": "powershell -ExecutionPolicy Bypass -File scripts/test-backend-integration.ps1",
    "test:backend:start": "npm run test:backend -- -StartBackend",
    "test:backend:verbose": "npm run test:backend -- -Verbose",
    "test:ui": "powershell -ExecutionPolicy Bypass -File scripts/test-ui-integration.ps1",
    "test:ui:start": "npm run test:ui -- -StartBackend",
    "test:ui:verbose": "npm run test:ui -- -Verbose",
    "test:ui:navigation": "npm run test:ui -- -TestPattern=navigation",
    "test:ui:forms": "npm run test:ui -- -TestPattern=forms",
    "test:ui:workflow": "npm run test:ui -- -TestPattern=workflow",
    "test:ui:accessibility": "npm run test:ui -- -TestPattern=accessibility",
    "test:ui:responsive": "npm run test:ui -- -TestPattern=responsive",
    "ci:verify": "npm run typecheck && npm run lint && npm run test --silent && npm run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@hookform/resolvers": "^5.2.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-data-grid": "^8.9.1",
    "@mui/x-date-pickers": "^8.9.0",
    "@supabase/supabase-js": "^2.52.1",
    "@tanstack/react-query": "^5.83.0",
    "@testing-library/user-event": "^14.6.1",
    "antd": "^5.26.6",
    "axios": "^1.10.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "lodash": "^4.17.21",
    "lucide-react": "^0.525.0",
    "quagga": "^0.12.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.61.1",
    "react-router-dom": "^6.20.0",
    "recharts": "^3.1.0",
    "tailwind-merge": "^3.3.1",
    "yup": "^1.6.1",
    "zod": "^4.0.9",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^4.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "terser": "^5.43.1",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.39.0",
    "vite": "^5.0.0"
  }
}
