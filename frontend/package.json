{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern=__tests__",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:visual": "jest --testPathPattern=visual",
    "test:performance": "jest --testPathPattern=performance",
    "test:accessibility": "jest --testPathPattern=accessibility",
    "test:all": "jest --testPathPattern='(unit|integration|e2e|visual|performance|accessibility)'",
    "test:quick": "jest --testPathPattern='(unit|integration)' --maxWorkers=2",
    "test:full": "jest --testPathPattern='(unit|integration|e2e|visual|performance|accessibility)' --coverage --verbose"
  },
