{
  "name": "valeoflow-enterprise",
  "version": "2.0.0",
  "description": "VALEO-Die NeuroERP Enterprise System mit Schwarm-Intelligenz",
  "main": "scripts/swarm-autonomous.js",
  "scripts": {
    "swarm:init": "node scripts/swarm-init.js",
    "swarm:develop": "node scripts/swarm-develop.js",
