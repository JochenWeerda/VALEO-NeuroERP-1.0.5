    "swarm:test": "cross-env SWARM_MODE=testing AI_ENABLED=true node scripts/swarm-autonomous.js",
    "swarm:prod": "cross-env SWARM_MODE=production AI_ENABLED=true QUALITY_THRESHOLD=0.95 DEPLOYMENT_STRATEGY=conservative node scripts/swarm-autonomous.js",
    "start": "npm run swarm:start",
    "dev": "npm run swarm:dev",
    "test": "npm run swarm:test",
    "build": "npm run swarm:autonomous",
    "deploy": "npm run swarm:deploy"
