{
  "name": "valeoflow-api-gateway",
  "version": "1.0.0",
  "description": "ValeoFlow Enterprise API Gateway - SAP Fiori Style ERP Gateway",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "build": "npm run build:gateway",
    "build:gateway": "webpack --config webpack.config.js",
    "docker:build": "docker build -t valeoflow-gateway .",
    "docker:run": "docker run -p 3000:3000 valeoflow-gateway"
  },
  "keywords": [
    "valeoflow",
    "api-gateway",
    "enterprise",
    "sap-fiori",
    "erp",
    "kong",
    "express"
  ],
  "author": "VALEO NeuroERP Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-slow-down": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "redis": "^4.6.10",
    "axios": "^1.6.0",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "joi": "^17.11.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "prom-client": "^15.0.0",
    "node-cron": "^3.0.3",
    "circuit-breaker-js": "^0.0.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "webpack": "^5.89.0",
